<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <preference for="Aheadworks\RequisitionLists\Api\Data\RequisitionListInterface" type="Aheadworks\RequisitionLists\Model\RequisitionList" />
    <preference for="Aheadworks\RequisitionLists\Api\Data\RequisitionListItemInterface" type="Aheadworks\RequisitionLists\Model\RequisitionList\Item" />
    <preference for="Aheadworks\RequisitionLists\Api\RequisitionListRepositoryInterface" type="Aheadworks\RequisitionLists\Model\RequisitionListRepository" />
    <preference for="Aheadworks\RequisitionLists\Api\RequisitionListItemRepositoryInterface" type="Aheadworks\RequisitionLists\Model\RequisitionListItemRepository" />
    <preference for="Aheadworks\RequisitionLists\Api\Data\RequisitionListSearchResultsInterface" type="Magento\Framework\Api\SearchResults" />
    <preference for="Aheadworks\RequisitionLists\Api\Data\RequisitionListItemSearchResultsInterface" type="Magento\Framework\Api\SearchResults" />
    <preference for="Aheadworks\RequisitionLists\Api\CartManagementInterface" type="Aheadworks\RequisitionLists\Model\Service\CartManagement" />
    <preference for="Aheadworks\RequisitionLists\Api\Data\GroupedItemOptionValueInterface" type="Aheadworks\RequisitionLists\Model\Product\Option\Grouped\ItemOptionValue"/>
    <preference for="Aheadworks\RequisitionLists\Api\CustomerManagementInterface" type="Aheadworks\RequisitionLists\Model\Service\CustomerService"/>
    <preference for="Aheadworks\RequisitionLists\Api\RequisitionListManagementInterface" type="Aheadworks\RequisitionLists\Model\Service\RequisitionListService"/>
    <preference for="Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolverInterface" type="Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver"/>

    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="aw_requisition_list_listing_data_source" xsi:type="string">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Grid\Collection</item>
                <item name="aw_requisition_list_item_listing_data_source" xsi:type="string">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item\Grid\Collection</item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver">
        <arguments>
            <argument name="resolvers" xsi:type="array">
                <item name="bundle" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver\Bundle</item>
                <item name="grouped" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver\Grouped</item>
                <item name="configurable" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver\Configurable</item>
                <item name="custom_options" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver\CustomOptions</item>
                <item name="downloadable" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\FieldResolver\Downloadable</item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Controller\RList\DownloadSample">
        <arguments>
            <argument name="sampleData" xsi:type="array">
                <item name="header" xsi:type="array">
                    <item name="0" xsi:type="string">sku</item>
                    <item name="1" xsi:type="string">qty</item>
                    <item name="2" xsi:type="string">bundle_option</item>
                    <item name="3" xsi:type="string">bundle_option_qty</item>
                    <item name="4" xsi:type="string">super_group</item>
                    <item name="5" xsi:type="string">super_attribute</item>
                    <item name="6" xsi:type="string">options</item>
                    <item name="7" xsi:type="string">links</item>
                </item>
                <item name="rows" xsi:type="array">
                    <item name="1" xsi:type="array">
                        <item name="0" xsi:type="string">24-WG080</item>
                        <item name="1" xsi:type="string">1</item>
                        <item name="2" xsi:type="string">24-WG085,24-WG081-blue,24-WG084,24-WG088</item>
                        <item name="3" xsi:type="string">1,1,1,1</item>
                    </item>
                    <item name="2" xsi:type="array">
                        <item name="0" xsi:type="string">24-MB04</item>
                        <item name="1" xsi:type="string">1</item>
                    </item>
                    <item name="3" xsi:type="array">
                        <item name="0" xsi:type="string">24-WG085_Group</item>
                        <item name="1" xsi:type="string">1</item>
                        <item name="2" xsi:type="string"></item>
                        <item name="3" xsi:type="string"></item>
                        <item name="4" xsi:type="string">24-WG085:1,24-WG086:1,24-WG087:1</item>
                    </item>
                    <item name="4" xsi:type="array">
                        <item name="0" xsi:type="string">MJ03</item>
                        <item name="1" xsi:type="string">1</item>
                        <item name="2" xsi:type="string"></item>
                        <item name="3" xsi:type="string"></item>
                        <item name="4" xsi:type="string"></item>
                        <item name="5" xsi:type="string">Size:XS,Color:Black</item>
                    </item>
                    <item name="5" xsi:type="array">
                        <item name="0" xsi:type="string">24-MB01</item>
                        <item name="1" xsi:type="string">1</item>
                        <item name="2" xsi:type="string"></item>
                        <item name="3" xsi:type="string"></item>
                        <item name="4" xsi:type="string"></item>
                        <item name="5" xsi:type="string"></item>
                        <item name="6" xsi:type="string">2:4,2:3</item>
                    </item>
                    <item name="6" xsi:type="array">
                        <item name="0" xsi:type="string">DOWNLOAD PROD</item>
                        <item name="1" xsi:type="string">1</item>
                        <item name="2" xsi:type="string"></item>
                        <item name="3" xsi:type="string"></item>
                        <item name="4" xsi:type="string"></item>
                        <item name="5" xsi:type="string"></item>
                        <item name="6" xsi:type="string"></item>
                        <item name="7" xsi:type="string">1,2</item>
                    </item>
                </item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Model\Import\PrepareImportData">
        <arguments>
            <argument name="prepareImportDataProcessors" xsi:type="array">
                <item name="csv" xsi:type="object">Aheadworks\RequisitionLists\Model\Import\ImportData\CsvImportDataProcessor</item>
            </argument>
        </arguments>
    </type>

    <type name="Magento\Framework\EntityManager\MetadataPool">
        <arguments>
            <argument name="metadata" xsi:type="array">
                <item name="Aheadworks\RequisitionLists\Api\Data\RequisitionListInterface" xsi:type="array">
                    <item name="entityTableName" xsi:type="const">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList::MAIN_TABLE_NAME</item>
                    <item name="identifierField" xsi:type="const">Aheadworks\RequisitionLists\Api\Data\RequisitionListInterface::LIST_ID</item>
                </item>
                <item name="Aheadworks\RequisitionLists\Api\Data\RequisitionListItemInterface" xsi:type="array">
                    <item name="entityTableName" xsi:type="const">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item::MAIN_TABLE_NAME</item>
                    <item name="identifierField" xsi:type="const">Aheadworks\RequisitionLists\Api\Data\RequisitionListItemInterface::ITEM_ID</item>
                </item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Ui\DataProvider\RequisitionList\ListingDataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Collection</argument>
        </arguments>
    </type>
    <type name="Aheadworks\RequisitionLists\Ui\DataProvider\RequisitionList\Item\ListingDataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item\Collection</argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Model\RequisitionList\Item\Options\ProcessorPool">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="bundle" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\Option\Bundle\Processor</item>
                <item name="configurable" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\Option\Configurable\Processor</item>
                <item name="custom_options" xsi:type="object">Magento\Catalog\Model\ProductOptionProcessor</item>
                <item name="downloadable" xsi:type="object">Magento\Downloadable\Model\ProductOptionProcessor</item>
                <item name="grouped" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\Option\Grouped\Processor</item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Model\Product\DetailProvider\Pool">
        <arguments>
            <argument name="providers" xsi:type="array">
                <item name="simple" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\DetailProvider\SimpleProvider</item>
                <item name="grouped" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\DetailProvider\GroupedProvider</item>
                <item name="bundle" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\DetailProvider\BundleProvider</item>
                <item name="configurable" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\DetailProvider\ConfigurableProvider</item>
            </argument>
        </arguments>
    </type>

    <type name="Aheadworks\RequisitionLists\Model\Product\View\Processor\RendererComposite">
        <arguments>
            <argument name="rendererList" xsi:type="array">
                <item name="image" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\View\Processor\Renderer\Image</item>
                <item name="options" xsi:type="object">Aheadworks\RequisitionLists\Model\Product\View\Processor\Renderer\Options</item>
            </argument>
        </arguments>
    </type>

    <type name="Magento\Framework\EntityManager\Operation\ExtensionPool">
        <arguments>
            <argument name="extensionActions" xsi:type="array">
                <item name="Aheadworks\RequisitionLists\Api\Data\RequisitionListItemInterface" xsi:type="array">
                    <item name="create" xsi:type="array">
                        <item name="requisitionListItemHandler" xsi:type="string">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item\Relation\RequisitionList\SaveHandler</item>
                    </item>
                    <item name="update" xsi:type="array">
                        <item name="requisitionListItemHandler" xsi:type="string">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item\Relation\RequisitionList\SaveHandler</item>
                    </item>
                    <item name="delete" xsi:type="array">
                        <item name="requisitionListItemHandler" xsi:type="string">Aheadworks\RequisitionLists\Model\ResourceModel\RequisitionList\Item\Relation\RequisitionList\SaveHandler</item>
                    </item>
                </item>
            </argument>
        </arguments>
    </type>

    <type name="Magento\Framework\App\Action\AbstractAction">
        <plugin name="awReqList" type="Aheadworks\RequisitionLists\Plugin\App\Action\ContextPlugin" />
    </type>
</config>
