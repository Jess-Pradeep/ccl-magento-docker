<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Ui\Block\Wrapper"
                   name="aw.ca.history.log"
                   template="Aheadworks_Ca::history_log/listing.phtml"
                   cacheable="false">
                <arguments>
                    <argument name="uiComponent" xsi:type="string">aw_ca_history_log_listing</argument>
                    <argument name="additionalClasses" xsi:type="string">aw_ca__history-log-listing</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
